    "ts-node": "^10.9.1",
    "typescript": "~3.9.7"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.186.0",
    "aws-cdk-lib": "2.45.0",
    "aws-lambda": "^1.0.7",
